{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "dest": "public/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "headers": [
      {
        "source": "/images/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }